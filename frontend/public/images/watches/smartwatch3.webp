<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 400">
  <!-- Background -->
  <rect width="400" height="400" fill="#f5f5f5"/>
  
  <!-- Smartwatch Case -->
  <circle cx="200" cy="200" r="140" fill="#1a1a1a" stroke="#333" stroke-width="2"/>
  <circle cx="200" cy="200" r="130" fill="#2a2a2a"/>
  
  <!-- Display -->
  <circle cx="200" cy="200" r="120" fill="#000"/>
  
  <!-- Digital Display -->
  <circle cx="200" cy="200" r="115" fill="#0a0a0a"/>
  
  <!-- Time Display -->
  <text x="200" y="190" font-family="monospace" font-size="44" font-weight="bold" fill="#E0A084" text-anchor="middle">10:45</text>
  
  <!-- Weather -->
  <text x="200" y="230" font-family="Arial" font-size="14" fill="#999" text-anchor="middle">☀️ 28°C</text>
  
  <!-- Battery -->
  <text x="200" y="260" font-family="Arial" font-size="12" fill="#666" text-anchor="middle">🔋 85%</text>
  
  <!-- Brand -->
  <text x="200" y="350" font-family="Arial" font-size="14" font-weight="bold" fill="#E0A084" text-anchor="middle">WEAR OS</text>
</svg>