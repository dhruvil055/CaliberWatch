<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 400">
  <!-- Background -->
  <rect width="400" height="400" fill="#f5f5f5"/>
  
  <!-- Watch Case Gold -->
  <circle cx="200" cy="200" r="180" fill="#daa520" stroke="#b8860b" stroke-width="2"/>
  <circle cx="200" cy="200" r="170" fill="#ffd700"/>
  
  <!-- Watch Glass -->
  <circle cx="200" cy="200" r="160" fill="#e8f4f8" opacity="0.5"/>
  
  <!-- Watch Face -->
  <circle cx="200" cy="200" r="155" fill="#f5e6d3"/>
  
  <!-- Hour Markers -->
  <g stroke="#8b7500" stroke-width="3">
    <line x1="200" y1="50" x2="200" y2="70"/>
    <line x1="290" y1="90" x2="278" y2="108"/>
    <line x1="350" y1="200" x2="330" y2="200"/>
    <line x1="290" y1="310" x2="278" y2="292"/>
    <line x1="200" y1="350" x2="200" y2="330"/>
    <line x1="110" y1="310" x2="122" y2="292"/>
    <line x1="50" y1="200" x2="70" y2="200"/>
    <line x1="110" y1="90" x2="122" y2="108"/>
  </g>
  
  <!-- Roman Numerals -->
  <text x="200" y="70" font-family="serif" font-size="14" font-weight="bold" fill="#8b7500" text-anchor="middle">XII</text>
  <text x="330" y="210" font-family="serif" font-size="14" font-weight="bold" fill="#8b7500" text-anchor="middle">III</text>
  <text x="200" y="340" font-family="serif" font-size="14" font-weight="bold" fill="#8b7500" text-anchor="middle">VI</text>
  <text x="70" y="210" font-family="serif" font-size="14" font-weight="bold" fill="#8b7500" text-anchor="middle">IX</text>
  
  <!-- Center Dot -->
  <circle cx="200" cy="200" r="6" fill="#8b7500"/>
  
  <!-- Hour Hand -->
  <line x1="200" y1="200" x2="200" y2="120" stroke="#8b7500" stroke-width="8" stroke-linecap="round"/>
  
  <!-- Minute Hand -->
  <line x1="200" y1="200" x2="260" y2="140" stroke="#8b7500" stroke-width="6" stroke-linecap="round"/>
  
  <!-- Watch Brand Text -->
  <text x="200" y="280" font-family="Arial" font-size="14" font-weight="bold" fill="#8b7500" text-anchor="middle">TITAN</text>
</svg>